[flake8]
max-line-length = 127
max-complexity = 10
ignore = 
    E203,
    W503,
    E501
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    .pytest_cache,
    cache,
    .cache,
    node_modules,
    migrations
per-file-ignores =
    __init__.py:F401
